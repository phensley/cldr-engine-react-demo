{
  "name": "cldr-engine-react-demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://phensley.github.io/cldr-engine-react-demo/",
  "dependencies": {
    "@phensley/cldr": "^0.9.1",
    "icepick": "^1.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-redux-router": "^0.0.14",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-saga": "^0.16.0",
		"wretch": "^1.1.1"
  },
  "scripts": {
    "build": "npm run packs && react-scripts-ts build",
    "clean": "rimraf ./public/packs",
    "deploy": "npm run build && npm run report && gh-pages -d build",
    "packs": "rimraf ./public/packs && node scripts/copy-packs.js",
    "report": "source-map-explorer --html -m build/static/js/main.*.js >build/report.html",
    "start": "npm run packs && react-scripts-ts start",
    "test": "npm run packs && react-scripts-ts test --env=jsdom"
  },
  "devDependencies": {
    "@types/icepick": "^1.3.0",
    "@types/jest": "^22.1.2",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.2.3",
    "@types/redux-saga": "^0.10.5",
    "dependency-cruiser": "^4.12.0",
    "gh-pages": "^1.1.0",
    "react-scripts-ts": "2.13.0",
    "rimraf": "^2.6.2",
    "source-map-explorer": "^1.5.0",
    "ts-node": "^7.0.0",
    "tslint-no-circular-imports": "^0.2.1",
    "typescript": "*"
  }
}
